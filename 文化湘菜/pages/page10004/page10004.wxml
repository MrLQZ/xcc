    <template is="zcText" data="{{data:text1, real_value:vessel_page_data.form_data.zxbt, detail_data: detail_data}}"></template>
  </block>
  <block wx:else>
    <template is="zcText" data="{{data:text1, real_value:item.form_data.zxbt, detail_data: detail_data}}"></template>
  </block>
</view>

